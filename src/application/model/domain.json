{
  "id": {
    "type": "string",
    "required": true
  },
  "description": {
    "type": "string",
    "nullable": true
  },
  "status": {
    "type": "string",
    "required": true,
    "allowed": [
      "pending",
      "active",
      "inactive",
      "suspended"
    ]
  },
  "ssl_enabled": {
    "type": "boolean"
  },
  "dns_records": {
    "type": "list",
    "schema": {
      "type": "string"
    },
    "default": []
  },
  "server": {
    "type": "string",
    "nullable": true
  },
  "container": {
    "type": "string",
    "nullable": true
  },
  "service": {
    "type": "string",
    "nullable": true
  },
  "created_at": {
    "type": "string"
  },
  "updated_at": {
    "type": "string"
  },
  "domain": {
    "type": "string",
    "required": true
  },
  "owner": {
    "type": "string"
  }
}